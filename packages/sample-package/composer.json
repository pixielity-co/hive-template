{
    "name": "phphive/sample-package",
    "description": "Sample package package",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "phpstan/phpstan": "^2.0",
        "laravel/pint": "^1.18"
    },
    "autoload": {
        "psr-4": {
            "PhpHive\\SamplePackage\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhpHive\\SamplePackage\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}